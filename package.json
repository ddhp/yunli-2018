{
  "name": "react-isomorphic-boilerplate",
  "version": "0.6.1",
  "main": "src/server/index.js",
  "license": "MIT",
  "scripts": {
    "start": "DEBUG=*,-nodemon*,-express*,-send,-babel*,-eslint*,-css-modules* NODE_ENV=hot babel-node --inspect src/server/hot/index.js",
    "build:browser:dev": "webpack --env=dev --config=webpack.browser.babel.js --profile --progress --json > compilation-stats.json",
    "build:server:dev": "webpack --env=dev --config=webpack.server.babel.js --progress --profile --colors",
    "build:browser:prod": "webpack --env=prod --config=webpack.browser.babel.js --progress --json > compilation-stats.json",
    "build:server:prod": "webpack --env=prod --config=webpack.server.babel.js --progress --profile --colors",
    "eslint": "eslint ./",
    "test": "npm run eslint && cross-env NODE_ENV=test nyc ava --verbose",
    "coverage": "nyc report --reporter=lcov",
    "report": "cross-env NODE_ENV=test nyc ava && yarn run coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "asset-require-hook": "^1.2.0",
    "autoprefixer": "^8.2.0",
    "ava": "^0.25.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "ignore-styles": "^5.0.1",
    "mini-css-extract-plugin": "^0.4.0",
    "mock-require": "^3.0.1",
    "node-sass": "^4.6.1",
    "nyc": "^11.3.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "^2.0.8",
    "remove-debug-loader": "^0.2.2",
    "reset.css": "^2.0.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "style-loader": "^0.20.1",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0",
    "animated-scroll-to": "^1.1.11",
    "babel-upgrade": "^1.0.1",
    "body-parser": "^1.18.2",
    "camelize": "^1.0.0",
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "immutability-helper": "^2.6.2",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-swipeable": "^4.2.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sqlite": "^4.0.14",
    "sqlite3": "^5.0.0",
    "superagent": "^3.8.2",
    "transform-runtime": "^0.0.0"
  },
  "description": "This boilerplate would help you build a react/redux/react-router isomorphic/universal web app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddhp/react-isomorphic-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "isomorphic",
    "universal",
    "hmr",
    "hot-reload",
    "react-router"
  ],
  "author": "ddhp",
  "bugs": {
    "url": "https://github.com/ddhp/react-isomorphic-boilerplate/issues"
  },
  "homepage": "https://github.com/ddhp/react-isomorphic-boilerplate#readme",
  "ava": {
    "files": [
      "src/**/__tests__/**/*.js",
      "src/**/*.spec.js",
      "!test/fixtures/**/*",
      "!test/helpers/**/*"
    ],
    "require": [
      "@babel/register",
      "ignore-styles",
      "./src/enzymeSetup.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
